{
  "scripts": {
    "dfx_install": "DFX_VERSION=0.15.1 sh -ci \"$(curl -fsSL https://sdk.dfinity.org/install.sh)\"",
    "replica_start": "dfx start --background",
    "replica_stop": "dfx stop",
    "canister_deploy_local": "dfx deploy parking_reservation_ICP",
    "canister_deploy_mainnet": "dfx deploy --network ic parking_reservation_ICP",
    "canister_uninstall": "dfx canister uninstall-code parking_reservation_ICP",
    "canister_call_get_message": "dfx canister call parking_reservation_ICP getMessage",
    "canister_call_set_message": "dfx canister call parking_reservation_ICP setMessage '(\"Hello world!\")'"
  },
  "dependencies": {
    "azle": "0.17.1",
    "typescript-eslint": "^0.0.1-alpha.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/uuid": "^9.0.5"
  }
}
